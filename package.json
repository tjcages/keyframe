{
  "name": "cloudflare-dev-platform",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm -r --parallel --filter @keyframe/react --filter @keyframe/web dev",
    "dev:web": "pnpm --filter @keyframe/web dev",
    "dev:lib": "pnpm --filter @keyframe/react dev",
    "build": "pnpm -r build",
    "start": "pnpm --filter @keyframe/web start",
    "shadcn-add": "pnpm dlx shadcn@latest add",
    "lint": "biome check",
    "format": "biome check --write && prettier -w \"**/*\" \"!**/*.{md,mdx}\" --ignore-unknown --cache",
    "format-unsafe": "biome format . --write --unsafe",
    "typecheck": "pnpm -r typecheck",
    "prepare": "lefthook install"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.5.3",
    "@astrojs/react": "^4.3.0",
    "@astrojs/sitemap": "^3.4.0",
    "@million/lint": "^1.0.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.7",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@xyflow/react": "^12.6.4",
    "astro": "^5.8.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.19.1",
    "gsap": "^3.13.0",
    "lucide-react": "^0.511.0",
    "motion": "^12.16.0",
    "next": "^15.4.6",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-svg-drawing": "^1.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-use-measure": "^2.1.7",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@tsconfig/strictest": "^2.0.5",
    "@tailwindcss/postcss": "^4.1.0",
    "@types/node": "^22.15.21",
    "@types/react-syntax-highlighter": "^15.5.13",
    "lefthook": "^1.11.13",
    "postcss": "^8.4.47",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.12",
    "tw-animate-css": "^1.3.0",
    "wrangler": "^4.16.1"
